{
    "manifest_version": 2,
    "name": "twitter-lists-desktop",
    "description": "A Chrome extension that allows for quick list-switching much like the mobile app.",
    "version": "0.1",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
    "page_action": {
        "default_icon": "icons/icon19.png"
      },
    "content_scripts": [{
        "matches": ["https://twitter.com/home"],
        "css": ["main.css"]
    }]
}